import java.util.* ;

import java.io.*; 

import java.util.ArrayList;

 

public class Solution 

{
public static void reverse(ArrayList<Integer>list,int left,int right){

        while(left<=right){

            int temp = list.get(left);

            list.set(left, list.get(right));

            list.set(right, temp);

            left++;

            right--;

        }

 

        

    }

    public static void reverseArray(ArrayList<Integer> a, int m)

    {

       int n=a.size();

       int arr[]=new int[n];

       

       reverse(a,m+1,n-1);

    }

}
