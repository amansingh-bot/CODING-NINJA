import java.util.* ;

import java.io.*; 

public class Solution {

    public static int countSetBits(int n) {

        // Write your code here.

        int rem=0;

        

        while(n>0)

        {

            if(n%2==1){

                rem+=1;

            }

            n/=2;

        

        }

        return rem;

 

    }

}
