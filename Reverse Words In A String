public class Solution 
{
	public static String reverseString(String str) 
	{
		//Write your code here
        

String sarr[]=str.split(" ");

        StringBuilder sb=new StringBuilder();

        for(int i=sarr.length-1;i>=0;i--){

            sb.append(sarr[i]+" ");

        }

        return sb.toString();
	}
}
